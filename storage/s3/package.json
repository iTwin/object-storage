{
  "name": "@itwin/object-storage-s3",
  "version": "1.3.0",
  "description": "Object storage implementation base for S3 compatible providers",
  "keywords": [
    "Bentley",
    "iTwin",
    "iTwin platform",
    "Object storage",
    "S3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/object-storage"
  },
  "homepage": "https://github.com/iTwin/object-storage/tree/main/storage/s3",
  "license": "MIT",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "https://www.bentley.com"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc 1>&2",
    "clean": "rimraf lib",
    "lint": "eslint ./src/**/*.ts 1>&2",
    "lint-fix": "eslint --fix ./src/**/*.ts 1>&2 && sort-package-json",
    "spell-check": "cspell \"**\" --config ./node_modules/@itwin/object-storage-common-config/cspell.json",
    "test:integration:backend": "",
    "test:integration:frontend": "",
    "test:unit": "npm run test:unit:backend && npm run test:unit:frontend",
    "test:unit:backend": "npm run test:unit:backend:common && npm run test:unit:backend:local",
    "test:unit:backend:common": "node lib/test/unit/backend/RunCommonUnitTests.js",
    "test:unit:backend:local": "mocha lib/test/unit/**/*.test.js --color --exit",
    "test:unit:frontend": "cypress run --browser chrome --config-file node_modules/@itwin/object-storage-common-config/cypress.json",
    "test:unit:frontend:debug": "cypress open --browser chrome --config-file node_modules/@itwin/object-storage-common-config/cypress.json"
  },
  "prettier": "@itwin/object-storage-common-config/prettier.json",
  "eslintConfig": {
    "extends": "./node_modules/@itwin/object-storage-common-config/.eslintrc.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.105.0",
    "@aws-sdk/client-sts": "3.105.0",
    "@aws-sdk/lib-storage": "3.105.0",
    "@aws-sdk/s3-request-presigner": "3.105.0",
    "@itwin/cloud-agnostic-core": "workspace:*",
    "@itwin/object-storage-core": "workspace:*",
    "inversify": "~5.0.1",
    "reflect-metadata": "~0.1.13"
  },
  "devDependencies": {
    "@aws-sdk/types": "3.78.0",
    "@itwin/object-storage-common-config": "workspace:*",
    "@itwin/object-storage-tests-backend-unit": "workspace:*",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "~16.6.2",
    "@types/sinon": "~10.0.11",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cspell": "~5.18.5",
    "cypress": "~9.5.4",
    "eslint": "~7.32.0",
    "mocha": "^9.2.1",
    "nyc": "^14.0.0",
    "rimraf": "^2.6.2",
    "sinon": "~14.0.0",
    "sort-package-json": "~1.53.1",
    "typescript": "~4.7.4"
  },
  "engines": {
    "node": ">=12.20 <17.0.0"
  }
}
